{
  "name": "sea-orange",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.dev next dev",
    "prod": "dotenv -e .env.prod next dev",
    "build": "dotenv -e .env.prod next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "next lint",
    "cz": "git add . && cz",
    "//": "新数据库部署",
    "migrate:dev": "dotenv -e .env.dev -- npx prisma migrate deploy",
    "migrate:prod": "dotenv -e .env.prod -- npx prisma migrate deploy",
    "//": "已有数据库同步",
    "migrateDiff:dev": "npx dotenv -e .env.dev  prisma migrate dev --name change",
    "migrateDiff:prod": "npx dotenv -e .env.prod  prisma migrate dev --name change"
  },
  "dependencies": {
    "@icon-park/react": "^1.4.2",
    "@pixi/graphics-extras": "^7.4.0",
    "@pixi/react": "^7.1.1",
    "@prisma/client": "^5.10.2",
    "antd": "^5.15.0",
    "matter-js": "^0.19.0",
    "matter-wrap": "^0.2.0",
    "next": "14.1.0",
    "pixi.js": "^7.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.5.0",
    "sass": "^1.71.1",
    "scss": "^0.2.4",
    "valtio": "^1.13.2"
  },
  "devDependencies": {
    "@types/matter-js": "^0.19.6",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "autoprefixer": "^10.4.18",
    "cz-git": "^1.9.0",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prisma": "^5.10.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "src/**/*.{ts,js,jsx,tsx,vue}": [
      "eslint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
