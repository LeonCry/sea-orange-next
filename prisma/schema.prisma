generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model sectionInfo {
  id          Int    @id @default(autoincrement())
  path        String
  title       String
  description String

  @@map(name: "section_info")
}

model visitTime {
  id        Int    @id @default(autoincrement())
  ip        String
  machine   String
  time      String
  path      String
  spendTime String

  @@map(name: "visit_time")
}
