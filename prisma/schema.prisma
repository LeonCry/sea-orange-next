generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model SectionInfo {
  id          Int    @id @default(autoincrement())
  path        String
  title       String
  description String

  @@map(name: "section_info")
}

model VisitTime {
  id        Int    @id @default(autoincrement())
  browser   String
  machine   String
  time      String
  path      String
  spendTime String

  @@map(name: "visit_time")
}
